<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_checked="true">
        <shape android:dither="true" android:visible="true" android:shape="rectangle" android:useLevel="false">
            <solid android:color="@color/switchColor"/>
            <corners android:radius="100dp"/>
            <size android:height="0dp" android:width="0dp"/>
            <stroke android:width="0dp" android:color="@color/switchColor"/>
        </shape>
    </item>
    <item android:state_enabled="false" android:state_checked="false" android:left="5dp" android:top="5dp" android:right="5dp" android:bottom="5dp">
        <shape android:dither="true" android:visible="true" android:shape="rectangle" android:useLevel="false">
            <solid android:color="@color/switchStroke"/>
            <corners android:radius="100dp"/>
            <size android:height="20dp" android:width="3dp"/>
            <stroke android:width="0dp" android:color="@color/switchStroke"/>
        </shape>
    </item>
</selector>
