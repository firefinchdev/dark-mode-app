<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_checked="true">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/cardViewBg" />
            <corners android:radius="100dp" />
            <size android:width="0dp" android:height="0dp" />
            <stroke android:width="0dp" android:color="@color/cardViewBg" />
        </shape>
    </item>
    <item android:bottom="5dp" android:left="5dp" android:right="5dp" android:state_checked="false" android:state_enabled="false" android:top="5dp">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/switchStroke" />
            <corners android:radius="100dp" />
            <size android:width="3dp" android:height="20dp" />
            <stroke android:width="0dp" android:color="@color/switchStroke" />
        </shape>
    </item>
</selector>
